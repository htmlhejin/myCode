<view>
	<view class="top">
		<image class="bgimg" src="../../assets/image/bgimg.png"></image>
		<view class="userinfo">
			<image class="avatar" src="{{user.avatarUrl}}"></image>
			<view class="username" bindtap="userinfo">
				<text class="paperOne">{{user.nickName}}</text>
				<text class="putong" wx:if="{{user.roleIds=='1'}}">超级管理会员</text>
				<text class="putong" wx:if="{{user.roleIds=='2'}}">会员用户</text>
				<text class="putong" wx:if="{{user.roleIds=='3'}}">普通用户</text>
			</view>
			<image class="icon" src="../../assets/image/right.png"></image>
		</view>
	</view>
	<view>
		<view class="vip">
			<image class="huiyuan" src="../../assets/image/huiyuan.png"></image>
			<text class="zhaunshu-vip">专属VIP会员</text>
			<view class="vip-rights">查看权益</view>
		</view>
	</view>
  	<view class="list-item-create">
			<view class="list-item-create-left" bindtap="iCreatePaper">
				<image class="list-img1" src="../../assets/image/chuangjianv.png"></image>
				<view class="create">我创建的试卷</view>
			</view>
			<view class="list-item-create-right" bindtap="iDonePaper">
				<image class="list-img2" src="../../assets/image/woyaozuode.png"></image>
				<view class="create">我做过的试卷</view>
			</view>
		</view>
	<view class="list"> 
		<view class="list-item">
			<image class="icon1" src="../../assets/image/icons/qianbao.png"></image>
			<text class="title">钱包</text>
			<text class="sub-title">钱包余额</text>
      	<text class="sub-title color-red">0</text>
			<text class="sub-title1">元</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
		<view class="list-item" bindtap="message">
			<image class="icon2" src="../../assets/image/icons/tixing.png"></image>
			<text class="title">消息</text>
			<text class="sub-title">您有一条新的消息</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
		<view class="list-item" bindtap="adviceBack">
			<image class="icon1" src="../../assets/image/icons/yijian.png"></image>
			<text class="title">意见与反馈</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
		<view class="list-item" bindtap="connect">
			<image class="icon1" src="../../assets/image/icons/kefu.png"></image>
			<text class="title">联系客服</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
		<view class="list-item">
			<image class="icon2" src="../../assets/image/icons/shezhi.png"></image>
			<text class="title">设置</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
		<view class="list-item list-item1" bindtap="about">
			<image class="icon2" src="../../assets/image/icons/guanyu.png"></image>
			<text class="title">关于我们</text>
			<image class="img-icon" src="../../assets/image/icons/arrows.png"></image>
		</view>
	</view>
</view>